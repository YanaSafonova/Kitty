#include <iostream>
#include <string>
#include <fstream>
#include <cmath>
const int size=1424;
using namespace std;
struct sms
{ string type;
String txt;
};
struct spam
{string name;
int index;
};
void create(ifstream &input,sms *a,size)
{a=new sms[size];
for (int i=0;i<size;i++)
input>>a[i].type<<a[i].txt;
}
